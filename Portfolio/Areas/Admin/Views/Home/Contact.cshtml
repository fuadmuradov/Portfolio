@model Contact
<div class="container">
    <div class="row">
        <div class="col-md-12 col-lg-12">
            <div class="card center-block mb-5" style="width:50rem;">
                <div class="card-body">
                    <form method="post" enctype="multipart/form-data">
                        <div asp-validation-summary="ModelOnly"></div>
                        <div class="form-group">
                            <label asp-for="@Model.Twitter" class="font-weight-600"></label>
                            <input asp-for="@Model.Twitter" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Twitter">  </span>
                        </div>
                        <div class="form-group">
                            <label asp-for="@Model.Linkedin" class="font-weight-600"></label>
                            <input asp-for="@Model.Linkedin" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Linkedin">  </span>
                        </div>
                        <div class="form-group">
                            <label asp-for="@Model.Instagram" class="font-weight-600"></label>
                            <input asp-for="@Model.Instagram" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Instagram">  </span>
                        </div>
                        <div class="form-group">
                            <label asp-for="@Model.Skype" class="font-weight-600"></label>
                            <input asp-for="@Model.Skype" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Skype">  </span>
                        </div>
                        <div class="form-group">
                            <label asp-for="@Model.Facebook" class="font-weight-600"></label>
                            <input asp-for="@Model.Facebook" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Facebook">  </span>
                        </div>
                        <div class="form-group">
                            <label asp-for="@Model.Location" class="font-weight-600"></label>
                            <input asp-for="@Model.Location" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Location">  </span>
                        </div>
                        <div class="form-group">
                            <label asp-for="@Model.Phone" class="font-weight-600"></label>
                            <input asp-for="@Model.Phone class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Phone">  </span>
                        </div>
                        <div class="form-group">
                            <label asp-for="@Model.Email" class="font-weight-600"></label>
                            <input asp-for="@Model.Email" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Email">  </span>
                        </div>
                      
                        <button type="submit" class="btn btn-success" onclick="return confirm('Are you sure you want to Update?')">Update</button>
                    </form>
                </div>
            </div>
        </div>
    </div>

</div>




